package day_09;

import java.util.Scanner;

public class ArTest {
/*
 * 1동
 * 1호 2호 3호
 * 
 */
	
	public static void main(String[] args) {
		int[] ho = new int[3];
		Scanner sc = new Scanner(System.in);		
		for(int i=0;i<ho.length;i++) {
			System.out.println((i+1) +"호 월세를 입력하세요");
			ho[i]=sc.nextInt();
			System.out.println(ho[i]);
		}
 	}  

 }
