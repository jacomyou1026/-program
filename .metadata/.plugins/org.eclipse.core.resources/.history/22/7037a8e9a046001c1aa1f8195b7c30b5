import javax.swing.JPanel;

public class Point extends JFrame{
	JPanel jp =new JPanel();
	JLabel jl2 = new JLabel("포인트");
	JLabel jl3 = new JLabel("사용 가능한 포인트");
	JLabel jl4 = new JLabel("전체내역");
	
	
	
	public Point() {
		setSize(500,700);
		setLayout(null);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
		setVisible(true);
		setTitle("포인트");
		jp.setLayout(null);
		jp.setBounds(, );
	}
}
