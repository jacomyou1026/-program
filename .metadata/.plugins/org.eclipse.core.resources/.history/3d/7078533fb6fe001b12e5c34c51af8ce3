package 백준;

import java.util.Scanner;

public class 백준_11021 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int count = 0;
		int num1 = 0;
		int num2 = 0;
		int sum []= new int[count];
		count = scan.nextInt();
		for (int i = 0; i < count; i++) {
			num1 = scan.nextInt();
			num2 = scan.nextInt();
			sum[i] = num1 + num2;
		}
		for (int i = 0; i < count; i++) {
			System.out.printf("Case #%d: %d\n",i+1,sum[i]);
		}
	}
}
